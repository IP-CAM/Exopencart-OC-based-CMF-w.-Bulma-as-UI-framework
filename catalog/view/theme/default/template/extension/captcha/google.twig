<script src="//www.google.com/recaptcha/api.js" type="text/javascript"></script>
<div class="field required captcha google-captcha">
  <label class="label">{{ entry_captcha }}</label>
  <div class="control">
    <div class="g-recaptcha" data-sitekey="{{ site_key }}"></div>
  </div>
  {% if error_captcha %}
  <div class="help is-danger">{{ error_captcha }}</div>
  {% endif %}
</div>
